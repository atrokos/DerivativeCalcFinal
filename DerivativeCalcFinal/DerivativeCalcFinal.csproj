<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="WpfMath" Version="0.11.0" />
    <PackageReference Include="AngouriMath" Version="1.4.0-preview.3" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Rules.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Rules.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Rules.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Rules.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
